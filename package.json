{
  "name": "basic_react_template",
  "repository": {
    "type": "git",
    "url": "https://github.com:yeraydiazdiaz/react-webpack-babel-template"
  },
  "version": "1.0.0",
  "description": "Basic React + Webpack + Babel template",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config ./webpack.config.prod.js",
    "build-watch": "webpack --config ./webpack.config.prod.js --watch",
    "dev": "webpack --config ./webpack.config.dev.js",
    "dev-watch": "webpack --config ./webpack.config.dev.js --watch",
    "start": "webpack-dev-server --progress --colors --hot --config ./webpack.config.dev.js",
    "test": "karma start",
    "test-watch": "karma start --auto-watch --no-single-run"
  },
  "keywords": [],
  "author": "Yeray Diaz Diaz",
  "license": "MIT",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "react"
    ],
    "plugins": ["@babel/plugin-proposal-class-properties"]
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.46",
    "@babel/preset-env": "^7.0.0-beta.46",
    "babel-loader": "^8.0.0-beta.2",
    "babel-preset-react": "^7.0.0-beta.3",
    "chai": "^3.0.0",
    "chai-enzyme": "^0.8.0",
    "cheerio": "^0.19.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.0.5",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-standard": "^3.0.1",
    "karma": "^2.0.2",
    "karma-firefox-launcher": "^1.1.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sinon-chai": "^1.3.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^3.0.0",
    "mobx": "^4.3.0",
    "mobx-react": "^5.1.2",
    "mocha": "^5.2.0",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-hot-loader": "^1.3.1",
    "react-test-renderer": "^15.6.2",
    "sinon": "^5.0.7",
    "sinon-chai": "^3.1.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.1"
  }
}
